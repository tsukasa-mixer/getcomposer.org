        "plausible-tracker": "^0.3",
        "prismjs": "^1.27.0"
    },
    "devDependencies": {
        "esbuild": "^0.13.15"
    },
    "scripts": {
        "dev": "esbuild js/app.js --bundle --outdir=web/build --sourcemap --loader:.gif=file --target=chrome58,firefox57,safari11,edge16 --watch",
